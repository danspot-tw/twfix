{
  "name": "Taiwan ID Fixer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Correct misguided errors in the name and flag of Taiwan",
  "homepage_url": "https://www.bing.com/",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["./twfix.js"]
    }
  ]
}